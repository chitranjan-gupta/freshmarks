{
  "manifest_version": 2,
  "name": "FreshMarks it!",
  "version": "1.0",
  "description": "A simple bookmark updater",
  "icons": {
    "19": "icons/star-filled-19.png",
    "38": "icons/star-filled-38.png",
    "48": "icons/star-filled-48.png",
    "96": "icons/star-filled-96.png"
  },
  "permissions": [
    "webRequest",
    "bookmarks",
    "tabs",
    "<all_urls>"
  ],
  "browser_action": {
    "default_icon": "icons/star-empty-38.png",
    "default_title": "FreshMarks it!"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}